            "eula_acceptance": {
                "additionalProperties": false,
                "description": "This EULA must be accepted as part of the initial deployment.\n\nRequired fields are:\n  * full_name\n  * job_title\n  * organization",
                "required": [
                    "full_name",
                    "job_title",
                    "organization"
                ],
                "title": "EULA Acceptance",
                "type": "object",
                "properties": {
                    "full_name": {
                        "default": "Jane Doe",
                        "description": "Full Name of the individual accepting the EULA.",
                        "examples": [
